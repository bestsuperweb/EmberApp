<!-- app/templates/admin/contacts.hbs -->

<h1>Contacts</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>E-mail</th>
      <th>Message</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |contact|}}
    <tr>
      <td>{{contact.id}}</td>
      <td>{{contact.email}}</td>
      <td>{{contact.message}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>